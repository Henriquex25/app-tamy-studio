{
    "expo": {
        "name": "app-tamy-studio",
        "slug": "app-tamy-studio",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "tamy-studio",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.henriquemiranda.apptamystudio"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.henriquemiranda.apptamystudio"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            "expo-secure-store",
            [
                "expo-font",
                {
                    "fonts": [
                        "./assets/fonts/PlaywriteGBS-Regular.ttf"
                    ]
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}